
@model  Laborator8App.Models.Team
@{
    ViewBag.Title = "Edit";
}
<div style=" color:black; background: rgb(69,53,53);
background: linear-gradient(254deg, rgba(69,53,53,1) 64%, rgba(115,54,54,1) 95%) !important">
    <center>
        <form method="post" action="/Teams/Edit/@Model.TeamId">
            @Html.HttpMethodOverride(HttpVerbs.Put)
            <br />
            @Html.HiddenFor(m => m.TeamId)
            <br />
            @Html.Label("TeamName", "Nume Echipa")
            <br />
            @Html.EditorFor(m => m.TeamName)
            @Html.ValidationMessage("TeamName", null, new { @class = "text-danger" })
            <br /><br />
            <button class="btn btn-sm btn-success" type="submit">Modifica echipa</button>
        </form>
    </center>
    </div>