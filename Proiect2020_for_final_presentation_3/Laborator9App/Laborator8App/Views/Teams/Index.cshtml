<h1>ECHIPE PE CARE LE CONDUCI</h1>


<ul class="tilesWrap">



    @foreach (var team in ViewBag.Teams)
    {
    <li>
        <div class="panel-body">

            <h3>@team.TeamName</h3>
        </div>

        <button>
            <a href="/Teams/Edit/@team.TeamId"> Editare echipa</a>
        </button>
        <button>
            <a href="/Teams/Show/@team.TeamId"> Vizualizare echipa</a>
        </button>
    </li>
}
</ul>
<br /><br />



<br />
<br />
<h1>ECHIPE DIN CARE FACI PARTE</h1>

<ul class="tilesWrap">
    @foreach (var team in ViewBag.TT)
    {
        <li>
            <div class="panel-body">

                <h3>@team.TeamName</h3>
            </div>

            <button>
                <a href="/Teams/Show/@team.TeamId"> Vizualizare echipa</a>
            </button>
        </li>
    }
</ul>
